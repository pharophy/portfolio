  <script async src="https://www.googletagmanager.com/gtag/js?id=UA-5893708-8"></script>
  <script>
    window.dataLayer = window.dataLayer || [];
    function gtag(){dataLayer.push(arguments);}
    gtag('js', new Date());

    gtag('config', 'UA-5893708-8');
  </script>
  </body>
</html>
